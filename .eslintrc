{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "indent": [2, 2, {
      "SwitchCase": 1
    }],
    "quotes": [2, "double"],
    "no-console": 0
  }
}
